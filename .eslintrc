{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "jsx": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true
  },
  "rules": {
    // Possible Errors
    "comma-dangle": 1,
    "no-cond-assign": 2,
    "no-console": 1,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 1,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "no-unexpected-multiline": 1,

    // Best Practices
    "consistent-return": 1,
    "default-case": 1,
    "dot-notation": 1,
    "eqeqeq": 2,
    "no-alert": 1,
    "no-caller": 2,
    "no-else-return": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 1,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 1,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 1,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-param-reassign": [2, {"props": true}],
    "no-process-env": 2,
    "no-proto": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 1,
    "no-useless-call": 2,
    "no-void": 2,
    "no-warning-comments": 1,
    "no-with": 2,
    "radix": 2,

    // Variables
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-undef-init": 1,
    "no-undef": 2,
    "no-undefined": 2,
    "no-unused-vars": 1,
    "no-use-before-define": 2,

    // Stylistic Issues
    "array-bracket-spacing": 1,
    // "block-spacing": [1, "never"],
    "brace-style": 1,
    "camelcase": 2,
    "comma-spacing": 1,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "eol-last": 1,
    "func-names": 2,
    "func-style": [2, "declaration"],
    "indent": [2, 2, {"SwitchCase": 2}],
    "key-spacing": 2,
    "lines-around-comment": 1,
    "linebreak-style": [2, "unix"],
    "new-cap": 2,
    "new-parens": 2,
    "no-lonely-if": 2,
    "no-multiple-empty-lines": 1,
    "no-nested-ternary": 1,
    "no-new-object": 2,
    "no-spaced-func": 1,
    "no-trailing-spaces": 1,
    "no-unneeded-ternary": 2,
    "quote-props": [2, "as-needed"],
    "quotes": [2, "double"],
    "semi": 2,
    "space-after-keywords": 1,
    "space-before-blocks": 1,
    "space-before-function-paren": [1, "never"],
    "space-in-parens": 1,
    "space-infix-ops": 1,
    "space-return-throw-case": 2,
    "space-unary-ops": 2,
    "wrap-regex": 1,

    // ECMAScript 6
    "arrow-spacing": 1,
    "constructor-super": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    // "no-dupe-class-members": 2,
    "no-this-before-super": 2,
    "no-var": 2,
    "object-shorthand": 2,
    // "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-spread": 2,
    // "prefer-template": 1

    // React
    "react/display-name": [1, {"acceptTranspilerName": true}],
    "react/jsx-curly-spacing": 1,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-undef": 2,
    "react/jsx-quotes": [2, "dobule"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/no-danger": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-did-update-set-state": 1,
    "react/no-unknown-property": 2,
    "react/prop-types": 1,
    "react/react-in-jsx-scope": 2,
    "react/require-extension": 2,
    "react/self-closing-comp": 2,
    "react/sort-comp": 1,
    "react/wrap-multilines": 2
  }
}
